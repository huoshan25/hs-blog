{
  "name": "hs-blog",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "packageManager": "pnpm@8.15.4",
  "scripts": {
    "dev": "turbo dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:blog-web": "pnpm --filter hs-blog-web dev",
    "dev:blog-admin": "pnpm --filter hs-blog-admin dev",
    "dev:blog-server": "pnpm --filter hs-blog-server dev",
    "dev:blog-portal": "pnpm --filter hs-blog-portal dev",
    "dev:shared": "pnpm --filter web-shared dev",
    "dev:docs": "pnpm --filter docs dev",
    "build:blog-web": "pnpm --filter hs-blog-web build",
    "build:blog-admin": "pnpm --filter hs-blog-admin build",
    "build:blog-server": "pnpm --filter hs-blog-server build",
    "build:blog-portal": "pnpm --filter hs-blog-portal build",
    "build:shared": "pnpm --filter web-shared build",
    "build:docs": "pnpm --filter docs build",
    "build": "turbo build",
    "build:all": "pnpm build:shared && pnpm build:docs && pnpm build:blog-web && pnpm build:blog-admin && pnpm build:blog-server && pnpm build:blog-portal",
    "clean": "turbo clean && rm -rf node_modules",
    "clean:deps": "rm -rf **/node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,vue,js,jsx,css,scss,md,json}\"",
    "lint": "turbo lint"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@vueuse/core": "^13.0.0",
    "cross-env": "^7.0.3",
    "turbo": "^2.5.2"
  },
  "devDependencies": {
    "prettier": "^3.2.5"
  }
}